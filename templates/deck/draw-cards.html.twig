{# test.html.twig #}

{% extends "base.html.twig"%}
{% block title %}Card | Draw cards{% endblock %}
{% block content %}

<div class="container-left">
    <h1>Draw some cards</h1>
    <div class="card" > 
        
        {% for card in data %}      
        <img src={{ asset(card.image) }}>
        {% endfor %}
        
    </div>
    <p>{{ cardleft}} card left in deck</p>
    <ul>
    <li><a href="{{ url("deck")}}">Deck</a></li>
    <li><a href="{{ url("shuffle")}}">Shuffle</a></li>
    <li><a href="{{ url("draw")}}">Draw</a></li>
    <li><a href="{{ url("deck2")}}">Joker</a></li>
    <li><a href="{{ link_to_deal }}">Poker deal</a></li>
    </ul>
</div>


{% endblock %}